<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="userinterfaz.datingdog.innovo.datingdog" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DogProfileActivity"
            android:label="DogProfileActivity" >
        </activity>
        <activity
            android:name=".RegisterProfileActivity"
            android:label="@string/title_activity_register_profile" >
        </activity>
        <activity
            android:name=".RegisterImageActivity"
            android:label="@string/title_activity_register_image" >
        </activity>
    </application>

</manifest>
